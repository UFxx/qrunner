{
	"name": "qrunner",
	"productName": "qrunner",
	"version": "0.0.1",
	"description": "qrunner",
	"main": "src/main.js",
	"private": true,
	"scripts": {
		"dev:renderer": "vite --config vite.renderer.config.mjs",
		"dev:main": "nodemon --watch src/main.js --watch src/preload.js --watch src/backend/ --ext js,json --exec \"electron .\"",
		"dev": "concurrently -n \"RENDERER,MAIN\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev:renderer\" \"npm run dev:main\"",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish"
	},
	"keywords": [],
	"author": {
		"name": "Dmitriy",
		"email": "smileprost@gmail.com"
	},
	"license": "MIT",
	"devDependencies": {
		"@electron-forge/cli": "^7.11.1",
		"@electron-forge/maker-deb": "^7.11.1",
		"@electron-forge/maker-rpm": "^7.11.1",
		"@electron-forge/maker-squirrel": "^7.11.1",
		"@electron-forge/maker-zip": "^7.11.1",
		"@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
		"@electron-forge/plugin-fuses": "^7.11.1",
		"@electron-forge/plugin-vite": "^7.11.1",
		"@electron/fuses": "^1.8.0",
		"@vitejs/plugin-vue": "^6.0.3",
		"electron": "40.1.0",
		"nodemon": "^3.1.11",
		"vite": "^5.4.21"
	},
	"dependencies": {
		"better-sqlite3": "^12.6.2",
		"concurrently": "^9.2.1",
		"electron-squirrel-startup": "^1.0.1",
		"sass": "^1.97.3",
		"vue": "^3.5.27"
	}
}
